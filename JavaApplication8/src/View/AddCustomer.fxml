      <Label layoutX="22.0" layoutY="146.0" text="Customer ID" />
      <Label layoutX="22.0" layoutY="177.0" text="Name" />
      <Label layoutX="22.0" layoutY="211.0" text="Address" />
      <Label layoutX="20.0" layoutY="277.0" text="State" />
      <Label layoutX="20.0" layoutY="243.0" text="Country" />
      <Label layoutX="22.0" layoutY="313.0" text="Zip Code" />
      <Label layoutX="22.0" layoutY="347.0" prefHeight="17.0" prefWidth="85.0" text="Phone Number" />
      <TextField fx:id="userIdText" disable="true" editable="false" layoutX="106.0" layoutY="142.0" prefHeight="25.0" prefWidth="195.0" promptText="Auto Generated" />
      <TextField fx:id="nameText" layoutX="106.0" layoutY="173.0" prefHeight="25.0" prefWidth="195.0" />
      <TextField fx:id="addressText" layoutX="106.0" layoutY="207.0" prefHeight="25.0" prefWidth="195.0" />
      <ComboBox fx:id="stateComboBox" layoutX="106.0" layoutY="273.0" prefHeight="25.0" prefWidth="195.0" promptText="Select a State/Providence" />
      <ComboBox fx:id="countryComboBox" layoutX="106.0" layoutY="239.0" onAction="#countryComboBoxPushed" prefHeight="25.0" prefWidth="195.0" promptText="Select a Country" />
      <TextField fx:id="zipCodeText" layoutX="106.0" layoutY="309.0" prefHeight="25.0" prefWidth="195.0" />
      <TextField fx:id="phoneNumberText" layoutX="106.0" layoutY="343.0" prefHeight="25.0" prefWidth="195.0" />
   </children>
</AnchorPane>
