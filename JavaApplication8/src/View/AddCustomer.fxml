<AnchorPane id="AnchorPane" prefHeight="553.0" prefWidth="346.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddCustomerController">
   <children>
      <Label layoutX="65.0" layoutY="30.0" text="ADD CUSTOMER">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button fx:id="saveButton" layoutX="22.0" layoutY="478.0" mnemonicParsing="false" onAction="#saveButtonPushed" prefHeight="61.0" prefWidth="146.0" text="SAVE" />
