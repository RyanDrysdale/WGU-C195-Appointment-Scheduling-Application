<AnchorPane id="AnchorPane" prefHeight="405.0" prefWidth="891.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustomersController">
   <children>
      <Button fx:id="mainMenuButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#mainMenuButtonPushed" prefHeight="19.0" prefWidth="90.0" text="Main Menu" />
      <TableView fx:id="customerTable" layoutX="17.0" layoutY="77.0" prefHeight="248.0" prefWidth="842.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="41.0" text="ID" />
          <TableColumn fx:id="customerNameColumn" prefWidth="146.0" text="Customer_Name" />
            <TableColumn fx:id="addressColumn" prefWidth="131.0" text="Address" />
            <TableColumn fx:id="phoneNumberColumn" minWidth="0.0" prefWidth="202.0" text="Phone Number" />
            <TableColumn fx:id="zipCodeColumn" prefWidth="126.0" text="Zip Code " />
            <TableColumn fx:id="divisionColumn" prefWidth="195.0" text="Division" />
        </columns>
      </TableView>
      <Label layoutX="330.0" layoutY="14.0" prefHeight="19.0" prefWidth="231.0" text="Customers">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Button fx:id="addButton" layoutX="17.0" layoutY="339.0" mnemonicParsing="false" onAction="#addButtonPushed" prefHeight="48.0" prefWidth="171.0" text="ADD" />
      <Button fx:id="updateButton" layoutX="360.0" layoutY="339.0" mnemonicParsing="false" onAction="#updateButtonPushed" prefHeight="48.0" prefWidth="171.0" text="UPDATE" />
      <Button fx:id="deleteButton" layoutX="688.0" layoutY="339.0" mnemonicParsing="false" onAction="#deleteButtonPushed" prefHeight="48.0" prefWidth="171.0" text="DELETE" />
   </children>
</AnchorPane>
